{
    "name": "roygoldman/composer-installers-discovery",
    "description": "A composer installers extension which permits dependencies to provide installer paths relative to the parent project.",
    "type": "composer-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Rich Gerdes",
            "email": "rich@richgerdes.com"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.1",
        "composer/installers": "^1.2"
    },
    "autoload": {
        "psr-4": {"RoyGoldman\\ComposerInstallersDiscovery\\": "src/"}
    },
    "extra": {
        "class": "RoyGoldman\\ComposerInstallersDiscovery\\Plugin"
    }
}
